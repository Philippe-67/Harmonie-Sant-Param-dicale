@model List<Rdv>

<h2>Liste des Rendez-vous</h2>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Nom du Patient</th>
            <th>Nom du Praticien</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var rdv in Model)
        {
            <tr>
                <td>@rdv.Date</td>
                <td>@rdv.NomPatient</td>
                <td>@rdv.NomPraticien</td>
            </tr>
        }
    </tbody>
</table>
